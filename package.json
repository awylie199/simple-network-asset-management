{
  "name": "simple-network-asset-management",
  "version": "0.0.1",
  "description": "A simple network asset management system",
  "main": "dist/bundle.js",
  "repository": "git@github.com:awylie199/simple-network-asset-management.git",
  "author": "Alexander Wylie <awylie199@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^10.12.*",
    "yarn": "^1.10.*",
    "npm": "^6.2.*"
  },
  "scripts": {
    "start": "export NODE_PATH=. && export $(cat .env) && yarn build && yarn start:server",
    "watch": "export NODE_PATH=. && export $(cat .env) && concurrently --kill-others \"npm:start:*\"",
    "start:watch": "export NODE_PATH=. && export $(cat .env) && webpack --config webpack.js --watch",
    "start:server": "forever server/index.js",
    "test": "export NODE_PATH=. && nyc mocha --opts mocha.opts \"client/**/*.spec.js\"",
    "test:watch": "export NODE_PATH=. && nyc mocha --opts mocha.opts --watch \"client/**/*.spec.js\"",
    "build": "export NODE_PATH=. && export $(cat .env) && webpack --config webpack.js --display-error-details --colors",
    "lint": "eslint server client -c .eslintrc",
    "lint:fix": "eslint server client -c .eslintrc --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "helmet": "^3.15.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^4.5.0",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-smart-import": "^0.7.6",
    "precss": "^3.1.2",
    "react-test-renderer": "^16.6.1",
    "react-test-utils": "^0.0.1",
    "redux-devtools-extension": "^2.13.5",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
