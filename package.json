    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "forever": "^0.15.3",
    "immutable": "^4.0.0-rc.12",
    "module-alias": "^2.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "ric": "^1.3.0",
    "sanitize-html-react": "^1.13.0",
    "spectre.css": "^0.5.5",
    "styled-components": "^4.0.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "winston": "^3.1.0"
  },
  "nyc": {
    "exclude": [
      "client/**/*.spec.js",
      "webpack.js",
      "webpack.test.js",
      "mocha.js"
    ]
  }
}
